/**
 * Illustrator scripts are prefixed with one of:
 * ⒈ ⒉ ⒊ ⒋ ⒌ ⒍ ⒎ ⒏ ⒐ ⒑ ⒒ ⒓ ⒔ ⒕ ⒖ ⒗ ⒘ ⒙ ⒚ ⒛
 */
{
    "version": "0.1",
    "configurations": [
        {
            "type": "extendscript-debug",
            "request": "launch",
            "name": "prepress-adobe-scripts",
            "program": "${workspaceFolder}/Illustrator Scripts/.incubating/Add Flap....jsx",
            //"program": "${workspaceFolder}/Photoshop Scripts/Images/Add Bleed to Images....jsx",
            "stopOnEntry": false
        }
    ]
}